    "import itertools\n",
    "\n",
    "componentes = [1, 2, 3]\n",
    "\n",
    "combinacoes = []\n",
    "for r in range(2, len(componentes) + 1):\n",
    "    combinacoes.extend(itertools.combinations(componentes, r))\n",
    "\n",
    "for c in combinacoes:\n",
    "    print(c)\n"
